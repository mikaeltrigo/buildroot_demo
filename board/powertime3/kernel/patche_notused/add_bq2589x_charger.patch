+	int ret;
+
+	ret = bq2589x_read_byte(bq, &val, BQ2589X_REG_0B);
+	if (ret < 0) {
+		dev_err(bq->dev, "%s Failed to read register 0x0b:%d\n", __func__, ret);
+		return ret;
+	}
+	val &= BQ2589X_CHRG_STAT_MASK;
+	val >>= BQ2589X_CHRG_STAT_SHIFT;
+	return val;
